{
  "name": "wordsolver",
  "version": "0.0.2",
  "description": "Solve anagrams and regex partterns using British English words",
  "engines": {
    "yarn": "^1.22"
  },
  "scripts": {
    "build": "yarn content && NODE_ENV=production ./node_modules/.bin/ts-node src/config/fuse.ts",
    "clean": "rm -f -r .cache/* dist/*",
    "content": "cp src/wordlist/2of4brif.txt dist",
    "deploy": "firebase deploy",
    "start": "yarn content && NODE_ENV=development ./node_modules/.bin/ts-node src/config/fuse.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/wordsolver"
  },
  "author": "stephen.d.francis@gmail.com",
  "license": "GPL-3.0-or-later",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.(ts|tsx)$",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "dependencies": {
    "koa": "^2.13",
    "koa-static": "^5.0",
    "react": "^16.13",
    "react-dom": "^16.13",
    "react-router": "^5.2",
    "react-router-dom": "^5.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0",
    "@types/node": "^14.14",
    "@types/react": "^17.0",
    "@types/react-dom": "^17.0",
    "@types/react-router": "^5.1",
    "@types/react-router-dom": "^5.1",
    "firebase-tools": "^9.20.0",
    "fuse-box": "^4.0.1-next.7",
    "jest": "^26.6",
    "nyc": "^15.1",
    "postcss-modules": "^4.1.3",
    "ts-jest": "^26.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.1"
  }
}
